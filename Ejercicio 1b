/ Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
//FUNCION DE ARREGLO
void Preciosminllamada(double precios[], int numPlanes){
    for (int idPlan= 0; idPlan < numPlanes; idPlan++){
        cout<<"Plan "<<idPlan<<" :  "<<precios[idPlan]<<endl;
    }
   
   
}

int main() {
    double precios[]={0.2, 0.5, 0.7, 1.0, 1.2};
    //TAMAÃ‘O
    int numPlanes= 5;
    Preciosminllamada(precios, numPlanes);
    return 0;
}
