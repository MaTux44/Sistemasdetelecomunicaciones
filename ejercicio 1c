void rellenar_Pais() {
   
}

void imprimir_Pais() {
    for (int i = 0; i < 195; i++) {
        std::cout << "idPais " << i << " -> " << Pais[i] << std::endl;
    }
}

int main() {


    auto inicio = std::chrono::high_resolution_clock::now();
    rellenar_Pais();
    auto medio = std::chrono::high_resolution_clock::now();
    imprimir_Pais();
    auto fin = std::chrono::high_resolution_clock::now();

    auto duracion1 = std::chrono::duration_cast<std::chrono::microseconds>(medio - inicio);
    auto duracion2 = std::chrono::duration_cast<std::chrono::microseconds>(fin - medio);

    std::cout << "Tiempo de Generacion de Paises: " << duracion1.count() << " microsegundos" << std::endl;
    std::cout << "Tiempo de Impresion de Paises: " << duracion2.count() << " microsegundos" << std::endl;

    return 0;
}
